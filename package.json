    "@vueuse/core": "^10.9.0",
    "pinia": "^2.1.7",
    "tauri-plugin-fs-watch-api": "https://github.com/tauri-apps/tauri-plugin-fs-watch#v1",
    "tauri-plugin-store-api": "^0.0.0",
    "tauri-plugin-window-state-api": "https://github.com/tauri-apps/tauri-plugin-window-state#v1",
